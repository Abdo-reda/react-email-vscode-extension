{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "outDir": ".",
    "lib": ["ES2022", "DOM"],
    "sourceMap": false,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "composite": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
